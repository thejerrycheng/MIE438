////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////* Getting User Inputs --ISR Attached inside the functions */////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////

void readPS2() {
  ps2x.read_gamepad(false, vibrate);          //read controller and set large motor to spin at 'vibrate' speed

  if (ps2x.Button(PSB_START))  {
    //will be TRUE as long as button is pressed
    // displayLoading();
    display.clearDisplay();
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println("Start is being held");
    display.display();
    //          displayLoading();
  }

  if (ps2x.Button(PSB_SELECT)) {
    display.clearDisplay();
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println("Select is being held");
    display.display();
    //          displayLoading();

  }


  if (ps2x.Button(PSB_PAD_UP)) {        //will be TRUE as long as button is pressed
    display.clearDisplay();
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println("Up held this hard: ");
    display.print(ps2x.Analog(PSAB_PAD_UP), DEC);
    display.display();
    snake_dir = FORWARD;
    //          displayLoading();

  }
  if (ps2x.Button(PSB_PAD_RIGHT)) {
    display.clearDisplay();
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println("Right held this hard: ");
    display.print(ps2x.Analog(PSAB_PAD_LEFT), DEC);
    display.display();
    snake_dir = RIGHT;
    //          displayLoading();

  }
  if (ps2x.Button(PSB_PAD_LEFT)) {
    display.clearDisplay();
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println("LEFT held this hard: ");
    display.print(ps2x.Analog(PSAB_PAD_LEFT), DEC);
    display.display();
    snake_dir = LEFT;
    //          displayLoading();

  }
  if (ps2x.Button(PSB_PAD_DOWN)) {
    display.clearDisplay();
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println("DOWN held this hard: ");
    display.print(ps2x.Analog(PSAB_PAD_DOWN), DEC);
    display.display();
    snake_dir = BACKWARD;
    //          displayLoading();

  }

  vibrate = ps2x.Analog(PSAB_BLUE);
  vibrate = ps2x.Analog(PSAB_RED);  //this will set the large motor vibrate speed based on
  //how hard you press the blue (X) button

  if (ps2x.NewButtonState())               //will be TRUE if any button changes state (on to off, or off to on)
  {
    if (ps2x.Button(PSB_L3)) {

      display.clearDisplay();
      display.setTextColor(WHITE);
      display.setTextSize(1);
      display.setCursor(0, 0);
      display.println("L3 pressed");
      display.display();
      //      displayLoading();


    }
    if (ps2x.Button(PSB_R3)) {
      display.clearDisplay();
      display.setTextColor(WHITE);
      display.setTextSize(1);
      display.setCursor(0, 0);
      display.println("R3 pressed");
      display.display();
      //            displayLoading();


    }
    if (ps2x.Button(PSB_L2)) {

      display.clearDisplay();
      display.setTextColor(WHITE);
      display.setTextSize(1);
      display.setCursor(0, 0);
      display.println("L2 pressed");
      display.display();
      //            displayLoading();


    }
    if (ps2x.Button(PSB_R2)) {

      display.clearDisplay();
      display.setTextColor(WHITE);
      display.setTextSize(1);
      display.setCursor(0, 0);
      display.println("R2 pressed");
      display.display();
      //            displayLoading();


    }
    if (ps2x.Button(PSB_GREEN)) {

      display.clearDisplay();
      display.setTextColor(WHITE);
      display.setTextSize(1);
      display.setCursor(0, 0);
      display.println("Triangle pressed");
      display.display();
      snake_dir = UP;
      //            displayLoading();
    }

  }


  if (ps2x.ButtonPressed(PSB_RED))   {
    //will be TRUE if button was JUST pressed
    display.clearDisplay();
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println("Circle just pressed");
    display.display();


    //            displayLoading();

  }

  if (ps2x.ButtonReleased(PSB_PINK))     {       //will be TRUE if button was JUST released
    display.clearDisplay();
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println("Square just released");
    display.display();
    //      displayLoading();
  }

  if (ps2x.NewButtonState(PSB_BLUE))    {       //will be TRUE if button was JUST pressed OR released
    display.clearDisplay();
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println("X just changed");
    display.display();
    snake_dir = DOWN;
    //          displayLoading();

  }


  if (ps2x.Button(PSB_L1) || ps2x.Button(PSB_R1)) // print stick values if either is TRUE
  {
    display.clearDisplay();
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println("Stick Values:");
    display.print(ps2x.Analog(PSS_LY), DEC);
    display.print(",");
    display.print(ps2x.Analog(PSS_LX), DEC);
    display.print(",");
    display.print(ps2x.Analog(PSS_RY), DEC);
    display.print(",");
    display.print(ps2x.Analog(PSS_RX), DEC);
    display.print(",");
    display.display();
    //      displayLoading();
  }

  delay(50);

}
